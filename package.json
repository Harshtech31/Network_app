{
  "name": "final-network",
  "version": "1.0.0",
  "description": "Complete social networking platform with React Native and AWS",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npx expo start",
    "build:web": "cd frontend && npx expo export:web",
    "build:apk": "node build-apk.js",
    "build:apk:local": "node build-apk.js local",
    "deploy:frontend": "node deploy-frontend.js",
    "deploy:backend": "cd network-infra && npx cdk deploy NetworkStagingStack --require-approval never",
    "deploy:all": "npm run deploy:backend && npm run deploy:frontend",
    "test:frontend": "cd frontend && npm test",
    "lint:frontend": "cd frontend && npx expo lint",
    "type-check": "cd frontend && npx tsc --noEmit",
    "git:push": "git add . && git commit -m \"ðŸš€ Auto-update: $(date)\" && git push origin master",
    "setup": "npm run setup:frontend && npm run setup:backend",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd network-infra && npm install",
    "clean": "npm run clean:frontend && npm run clean:backend",
    "clean:frontend": "cd frontend && rm -rf node_modules dist .expo",
    "clean:backend": "cd network-infra && rm -rf node_modules cdk.out"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Harshtech31/Network_final.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "social-network",
    "aws",
    "lambda",
    "mobile-app",
    "typescript",
    "nodejs"
  ],
  "author": "Harshtech31",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Harshtech31/Network_final/issues"
  },
  "homepage": "https://github.com/Harshtech31/Network_final#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@expo/cli": "^0.17.0",
    "eas-cli": "^7.0.0"
  }
}
